/* 水平间距 */
.settle-content {
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 999;
  width: 100%;
  height: 50px;
  background-color: #fefefe;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 5px;
}
.settle-content .radio {
  display: flex;
  align-items: center;
}

